{
    "name": "tree-sitter-scd",
    "version": "1.0.0",
    "description": "SCD (Smart Contract Definition) grammar for tree-sitter",
    "main": "bindings/node",
    "types": "bindings/node",
    "keywords": [
        "tree-sitter",
        "scd",
        "smart-contract"
    ],
    "author": "Yurii Biurher",
    "license": "Apache-2.0",
    "dependencies": {
        "node-addon-api": "^8.5.0",
        "node-gyp-build": "^4.8.4"
    },
    "devDependencies": {
        "tree-sitter-cli": "^0.25.10"
    },
    "peerDependencies": {
        "tree-sitter": "^0.25.0"
    },
    "peerDependenciesMeta": {
        "tree-sitter": {
            "optional": true
        }
    },
    "scripts": {
        "build": "tree-sitter build --wasm",
        "test": "tree-sitter test",
        "install": "node-gyp-build",
        "prebuildify": "prebuildify --napi --strip",
        "playground": "tree-sitter playground"
    },
    "files": [
        "grammar.js",
        "binding.gyp",
        "prebuilds/**",
        "bindings/node/*",
        "queries/*",
        "src/**"
    ],
    "tree-sitter": [
        {
            "scope": "source.scd",
            "file-types": [
                "scd"
            ]
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ZoralLabs/tree-sitter-scd.git"
    },
    "bugs": {
        "url": "https://github.com/ZoralLabs/tree-sitter-scd/issues"
    },
    "homepage": "https://github.com/ZoralLabs/tree-sitter-scd#readme"
}
